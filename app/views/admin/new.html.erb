<h1>Create User here</h1>

<div class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Email</th>
        <th>Batch Year</th>
        <th>Batch Month</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%= form_for :user do |t|  %>
        <tr>
          <td>
            <%= t.text_field :login_email %>
          </td> 
          <td>
            <%= t.text_field :batch_year %>
          </td> 
          <td>
            <%= t.text_field :batch_month %>
          </td>
          <td>
            <%= t.submit "Create User!"%> 
          </td>
        </tr>

      <% end %>
    </tbody>
  </table>
</div>